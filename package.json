{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "babel-node-starter",
  "scripts": {
    "build": "rimraf dist && cross-env BABEL_ENV=release babel src --out-dir dist",
    "serve": "cross-env NODE_ENV=release node ./dist/index.js",
    "start": "nodemon --inspect --exec 'cross-env BABEL_ENV=debug NODE_ENV=debug babel-node --nolazy ./src/index.js'",
    "lint": "eslint .",
    "test": "rimraf coverage && jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.2.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.3.0",
    "eslint-config-alloy": "^3.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
